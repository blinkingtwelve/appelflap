<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="org.nontrivialpursuit.appelflap.p2pmonitor.PreflightFragment"
        android:label="Flight checks"
        tools:layout="@layout/fragment_p2pmonitor_preflight" />

    <fragment
        android:id="@+id/navigation_prefs"
        android:name="org.nontrivialpursuit.appelflap.p2pmonitor.PrefsFragment"
        android:label="Preferences"
        tools:layout="@layout/fragment_p2pmonitor_prefs" />

    <fragment
        android:id="@+id/navigation_status"
        android:name="org.nontrivialpursuit.appelflap.p2pmonitor.StatusFragment"
        android:label="Status"
        tools:layout="@layout/fragment_p2pmonitor_status" />

    <fragment
        android:id="@+id/navigation_logs"
        android:name="org.nontrivialpursuit.appelflap.p2pmonitor.LogFragment"
        android:label="Logs"
        tools:layout="@layout/fragment_p2pmonitor_log_list" />
</navigation>